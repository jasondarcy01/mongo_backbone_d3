  // var dataset = {price:720000, zipcode:11222, price:1500000, zipcode:11211, price:5500000, zipcode:10012, price:2500000, zipcode:10002};

  var width = 600;
  var height = 250;

  var dataset = [720000, 1500000, 5500000, 2500000];
  var zipcodeset = [11222, 11211, 10012, 10002];

  var xScale = d3.scale.ordinal()
                  .domain(d3.range(dataset.length))
                  .rangeRoundBands([0,width], 0.05);

  var yScale = d3.scale.linear()
                  .domain([0, d3.max(dataset)])
                  .range([0, height]);

      var svg = d3.select("body")
                    .append("svg")
                    .attr("width", width)
                    .attr("height", height);

      var barchart = svg.selectAll("rect")
                     .data(dataset)
                     .enter()
                     .append("rect")
                     .attr("x", function(d, i) {
                          return xScale(i);
                     })
                     .attr("y", function(d){
                          return height - yScale(d);
                     })
                      .attr("width", xScale.rangeBand())
                      .attr("height", function(d) {
                          return yScale(d);
                      });



</script>






